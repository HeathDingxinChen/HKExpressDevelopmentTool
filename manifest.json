{
  "name": "HKExpressDevelopmentTool",
  "description": "HKExpress Development Tool",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png"
  },
  "permissions": [
    "tabs","storage"
  ]
}